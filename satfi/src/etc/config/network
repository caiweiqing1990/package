
config interface 'loopback'
	option ifname 'lo'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'

config globals 'globals'
	option ula_prefix 'fdb9:a329:6e6d::/48'

config interface 'lan'
	option ifname 'eth0.1'
	option force_link '1'
	option type 'bridge'
	option proto 'static'
	option ipaddr '192.168.1.1'
	option netmask '255.255.255.0'
	option ip6assign '60'
	option macaddr '00:11:22:33:44:55'

config interface 'wan0'
	option _orig_ifname '@wan'
	option _orig_bridge 'false'
	option proto '3g'
	option device '/dev/ttyGPRS'
	option service 'umts'
	option apn '3GNET'
	option auto '0'
	option defaultroute '0'

config interface 'wan6'
	option _orig_ifname '@wan'
	option _orig_bridge 'false'
	option proto '3g'
	option device '/dev/ttyACM0'
	option service 'umts'
	option apn 'get'
	option auto '0'
	option keepalive '300 5'
	option defaultroute '0'

config interface 'wan8'
	option _orig_ifname '@wan'
	option _orig_bridge 'false'
	option proto '3g'
	option device '/dev/ttySAT0'
	option service 'umts'
	option apn 'get'
	option auto '0'
	option defaultroute '0'

config interface 'wwan'
	option proto 'dhcp'

